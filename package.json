{
  "name": "nuxthub-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "deploy": "nuxthub deploy",
    "postinstall": "nuxt prepare && tsdown scripts/translate-content.ts",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "translate": "node dist/translate-content.js",
    "translate:check": "node dist/translate-content.js list",
    "translate:publish": "node dist/translate-content.js publish"
  },
  "dependencies": {
    "@iconify/json": "^2.2.357",
    "@nuxt/content": "^3.6.3",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/i18n": "9.5.6",
    "@pinia/nuxt": "^0.11.1",
    "@unocss/extractor-mdc": "^66.3.3",
    "@unocss/nuxt": "^66.3.3",
    "@unocss/preset-icons": "^66.3.3",
    "better-sqlite3": "^12.2.0",
    "motion-v": "^1.5.0",
    "nuxt": "^4.0.0",
    "ogl": "^1.0.11"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.5.2",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "c12": "^2.0.4",
    "eslint": "^9.30.1",
    "gray-matter": "^4.0.3",
    "happy-dom": "^18.0.1",
    "nuxthub": "^0.9.2",
    "tsdown": "^0.12.9",
    "tsx": "^4.20.3",
    "unocss": "^66.3.3",
    "unocss-preset-theme": "^0.14.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12",
    "wrangler": "^4.24.3"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "unrs-resolver",
      "vue-demi",
      "workerd"
    ]
  }
}
