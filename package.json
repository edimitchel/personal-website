{
  "name": "nuxthub-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "deploy": "nuxthub deploy",
    "postinstall": "nuxt prepare && tsdown scripts/translate-content.ts",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "translate": "node dist/translate-content.js",
    "translate:check": "node dist/translate-content.js list",
    "translate:publish": "node dist/translate-content.js publish"
  },
  "dependencies": {
    "@iconify/json": "^2.2.353",
    "@nuxt/content": "^3.6.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.10.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "9.5.6",
    "@pinia/nuxt": "^0.11.1",
    "@unocss/nuxt": "^66.3.2",
    "@unocss/preset-icons": "^66.3.2",
    "better-sqlite3": "^12.1.1",
    "nuxt": "^3.17.5"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "c12": "^2.0.1",
    "eslint": "^9.29.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^18.0.1",
    "nuxthub": "^0.9.0",
    "tsdown": "^0.12.9",
    "tsx": "^4.7.1",
    "unocss": "^66.3.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.22.0"
  },
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "better-sqlite3"
    ]
  }
}