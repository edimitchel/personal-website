{
  "name": "nuxthub-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "deploy": "nuxthub deploy",
    "postinstall": "nuxt prepare && tsdown scripts/translate-content.ts --tsconfig scripts/tsconfig.json",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "translate": "node dist/translate-content.js",
    "translate:check": "node dist/translate-content.js list",
    "translate:publish": "node dist/translate-content.js publish"
  },
  "dependencies": {
    "@iconify/json": "^2.2.378",
    "@nuxt/content": "^3.6.3",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/image": "^1.11.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge",
    "@pinia/nuxt": "^0.11.2",
    "@unocss/extractor-mdc": "^66.4.2",
    "@unocss/nuxt": "^66.4.2",
    "@unocss/preset-icons": "^66.4.2",
    "better-sqlite3": "^12.2.0",
    "motion-v": "^1.7.0",
    "nuxt": "^4.0.3",
    "nuxt-og-image": "5.1.9",
    "nuxt-schema-org": "5.0.6",
    "nuxt-seo-utils": "7.0.14",
    "ogl": "^1.0.11"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "c12": "^3.2.0",
    "eslint": "^9.34.0",
    "gray-matter": "^4.0.3",
    "happy-dom": "^18.0.1",
    "nuxthub": "^0.9.3",
    "tsdown": "^0.13.5",
    "tsx": "^4.20.5",
    "unocss": "^66.4.2",
    "unocss-preset-theme": "^0.14.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6",
    "wrangler": "^4.32.0"
  },
  "resolutions": {
    "@nuxtjs/mdc": "^0.17.2"
  },
  "packageManager": "pnpm@10.15.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "unrs-resolver",
      "vue-demi",
      "workerd"
    ]
  }
}
